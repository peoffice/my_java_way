<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
	<class name="mark.workspace.heberate_toturial.Order" table="orders" catalog="classicmodels">
		<id name="orderNumber" type="java.lang.Integer">
			<column name="orderNumber" />
		</id>

		<property name="orderDate" type="java.util.Date">
			<column name="orderDate"/>
		</property>
		<property name="requiredDate" type="java.util.Date">
			<column name="requiredDate"/>
		</property>
		<property name="shippedDate" type="java.util.Date">
			<column name="shippedDate"/>
		</property>
		<property name="status" type="java.lang.String">
			<column name="status"/>
		</property>
		<property name="customerNumber" type="java.lang.Integer">
			<column name="customerNumber"/>
		</property>
	</class>
</hibernate-mapping>